[nmp]
    ROWS = 499  # grid row size
    COLS = 499  # grid col size
    Z = 4  # channel buffer size
    I = 20  # iterations
    S = 2  # 0 = global sync, 1 = local sync, 2 = async
    TRACE = false  # tracing
    RESULTS = false # results as traces
	COMPLETIONS = true # print completion timings for each proxel
    ORACLE = 1   # cpu spinwait per message, *0.25 ms ?
    DELAY1 = 30  # delay to N
    DELAY2 = 30 # delay to E
    DELAY3 = 150  # delay to S
    DELAY4 = 150  # delay to W
